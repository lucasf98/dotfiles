[alias]
    last = log -1 HEAD
    lo = log --oneline --graph --decorate -10
    la = log --oneline --graph --decorate --all -10
    laa = log --oneline --graph --decorate --all
    co = checkout
    cob = checkout -b
    br = branch
    brd = branch -d
    st = status
    ci = commit
    ls = ls-tree -r master --name-only
    unstage = reset HEAD --
    whatadded = log --diff-filter=A --
    pushreview = push origin HEAD:refs/for/master

[core]
    excludesFile = ~/.gitignore

[include]
    path = ~/.gitconfig.local

[credential]
    helper = cache --timeout=360000

[color]
    ui = auto
